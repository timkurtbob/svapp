<div class="document-list attachment-list">
  <% @attachments.sort! %>
  <% @attachments.reverse! %>
  <% documents = @attachments.select{ |a| a.file_type == 'document'} %>
    <% documents.each do |d| %>
    <%= link_to "#{d.name}", target: :_blank do %>
      <% fn = "#{d.name.to_s[60..110]}..."  %>
      <h3><%= fn %></h3>
    <% end %>
  <% end %>
  <div class="stop-attach-float">
  </div>
</div>
